{
  "name": "flexoki-light",
  "type": "light",
  "colors": {
    "editor.background": "#F2F0E5",
    "editor.foreground": "#100F0F",
    "editorLineNumber.foreground": "#878580",
    "editorLineNumber.activeForeground": "#100F0F"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6F6E69",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.quoted", "punctuation.definition.string"],
      "settings": {
        "foreground": "#24837B"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#5E409D"
      }
    },
    {
      "scope": ["constant.language", "constant.language.boolean"],
      "settings": {
        "foreground": "#5E409D"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier", "keyword.control", "keyword.operator.new", "keyword.operator.expression", "keyword.operator.logical"],
      "settings": {
        "foreground": "#66800B"
      }
    },
    {
      "scope": ["keyword.control.import", "keyword.control.export", "keyword.control.from", "meta.import", "meta.export"],
      "settings": {
        "foreground": "#A02F6F"
      }
    },
    {
      "scope": ["keyword.operator", "punctuation"],
      "settings": {
        "foreground": "#6F6E69"
      }
    },
    {
      "scope": ["entity.name.function", "support.function", "meta.function-call"],
      "settings": {
        "foreground": "#205EA6"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.class", "support.type"],
      "settings": {
        "foreground": "#AD8301"
      }
    },
    {
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "foreground": "#AD8301"
      }
    },
    {
      "scope": ["variable", "variable.parameter", "variable.other"],
      "settings": {
        "foreground": "#100F0F"
      }
    },
    {
      "scope": ["variable.language.this", "variable.language.self", "variable.language.super"],
      "settings": {
        "foreground": "#66800B",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#66800B"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#AD8301"
      }
    },
    {
      "scope": ["support.type.property-name", "meta.object-literal.key"],
      "settings": {
        "foreground": "#205EA6"
      }
    },
    {
      "scope": ["variable.other.property", "variable.other.object.property"],
      "settings": {
        "foreground": "#100F0F"
      }
    },
    {
      "scope": ["support.constant", "constant.other"],
      "settings": {
        "foreground": "#5E409D"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#66800B",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#205EA6"
      }
    },
    {
      "scope": ["entity.name.function.member"],
      "settings": {
        "foreground": "#205EA6"
      }
    }
  ]
}
